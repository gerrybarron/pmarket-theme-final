<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="css/customcss.css" />
    </head>
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo-header.png</argument>
                <argument name="logo_alt" xsi:type="string">Pmarket</argument>
                <argument name="logo_img_width" xsi:type="number">175</argument> 
                <argument name="logo_img_height" xsi:type="number">49</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>